phase1 <- read_excel("C:/Users/ndeye/OneDrive/Documents/SAE DPDT/poids.xlsx",sheet = 'Poids_phase_1', na = c("", "NA", "NaN", " ") )
#Phase 2 correpond au moment du régime pendant laquelle le sujet se pése qu'une seule fois par jour
phase2 <- read_excel("C:/Users/ndeye/OneDrive/Documents/SAE DPDT/poids.xlsx",sheet = 'Poids_phase_2', na = c("", "NA", "NaN", " ") )
##1.2 Nettoyage et conversion pour la phase 1
#a) Suppression des lignes où la colonne poids est manquante (NA)
phase1_clean <- phase1 %>%
filter(!is.na(Poids)) %>%
filter(!is.na(Heure)) #Supprimer aussi les lignes sans heure
#b) Conversion de la colonne heure par exemple 09H30 en heure décimale est égale à 9.5
#La fonction pour extraire les heures et minutes; calculer aussi l'heure en décimale
#Elle gére le format "HhMM" ou "HHhMM" et "0h00" qui est minuit
convertir_heure_decimale <- function(heure_str) {
#On utilise str_match pour extraire les chiffres avant 'h' (heure) et aprés 'h' (minutes)
match <- str_match(heure_str, "(\\d+?)h(\\d+)")
#Si l'extraction est réussit et les valeurs existent
if (nrow(match) > 0 && !is.na(match[1, 2]) && !is.na(match[1, 3])) {
heure <- as.numeric(match[1, 2])
minute <- as.numeric(match[1, 3])
#Gérer le cas spécial '0h00' pour minuit
if (heure_str == "0h00") {
return(24.0) #On met que la plage horaire soit cohérent pour le lissage
} else {
return(heure + minute / 60)
}
} else {
return(NA) #Retourne NA si le format n'est pas bon
}
}
#Appliquer la fonction pour créer la nouvelle colonne 'Heure_Decimale'
phase1_clean <- phase1_clean %>%
rowwise() %>% #Apllique ligne par ligne
mutate(Heure_Decimale = convertir_heure_decimale(Heure)) %>%
ungroup() %>% #Annule le rowwise
#Maintenant supprimons les lignes où la conversion a échoué , au cas où
filter(!is.na(Heure_Decimale)) %>%
#Selectionner les collonnes pertinantes
select(Jour, Heure, Poids, Heure_Decimale)
##1.3 Nettoyage et conversion pour la phase 1
