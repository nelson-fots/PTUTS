geom_hline(yintercept = moyenne_journaliere_lissee,
linetype = "dashed", color = "red", linewidth = 1) +
labs(
title = "Lissage LOESS du poids au cours de la journée (phase 1)",
x = "Heure (décimale)",
y = "Poids (kg)",
subtitle = paste("Moyenne journalière lissée =", round(moyenne_journaliere_lissee, 2), "kg")
