library(ggplot2)
x <- c(0, 2, 5, 10)
y <- c(30000, 35000, 45000, 60000)
ggplot(x , y, type = "l", xlab = "Années d'expériences", ylab = "Salaires brutes annuelles en €", main = "Estimation du salaire brute annuelle d'un analyste en fraude monétique")
library(ggplot2)
donnes <- data.frame(
x = c(0, 2, 5, 10, 15, 20)
y = c(30000, 35000, 45000, 60000, 69000, 73000)
library(ggplot2)
donnes <- data.frame(
experience = c(0, 2, 5, 10, 15, 20),
salaire_brut = c(30000, 35000, 45000, 60000, 69000, 73000)
)
# Graphique simple
ggplot(donnees, aes(x = experience, y = salaire_brut)) +
geom_point(color = "blue", size = 3) +  # Points
geom_smooth(method = "lm", se = FALSE, color = "red") +  # Ligne de tendance
labs(
title = "Estimation du salaire d'un analyste en fraude monétique",
x = "Années d'expérience",
y = "Salaire brut (€)"
) +
theme_minimal()
